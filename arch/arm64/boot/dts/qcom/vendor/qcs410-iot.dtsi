#include "qcs610-iot.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. QCS410 IOT";
	compatible = "qcom,qcs410-iot", "qcom,qcs410", "qcom,iot";
};

&usb1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&usb_typea_host_en>;
	qcom,default-mode-host;
	qcom,ignore-wakeup-src-in-hostmode;
};

&L19A {
       regulator-max-microvolt = <3304000>;
       qcom,init-voltage = <3300000>;
       regulator-always-on;
       qcom,init-mode = <RPMH_REGULATOR_MODE_HPM>;
};

&aliases {
	spi2 = &qupv3_se2_spi;
	spi6 = &qupv3_se6_spi;
};

&qupv3_se2_spi {
	status = "ok";
	spidev@1 {
		status = "ok";
		compatible = "qcom,spidev";
		reg = <0>;
		spi-max-frequency = <50000000>;
	};
};

&qupv3_se6_spi {
	status = "ok";
	spidev@2 {
		status = "ok";
		compatible = "qcom,spidev";
		reg = <0>;
		spi-max-frequency = <50000000>;
	};
};

&pm6150_charger {
	status = "disabled";
};

&pm6150_qg {
	status = "disabled";
};

&pm6150_bcl {
	status = "disabled";
};

&wdog {
	status = "disabled";
};

&qupv3_se1_i2c {
	/delete-property/ qcom,i2c-touch-active;
	focaltech@38 {
		status = "disabled";
	};
	lt,lt9611@3b {
		status = "disabled";
	};
};

&emac_gdsc {
	status = "disabled";
};

#include "qcs410-iot-pinctrl.dtsi"
#include "display/denso-sde.dtsi"
#include "camera/denso-camera.dtsi"
#include "sm6150-ext-codec-audio-overlay.dtsi"
