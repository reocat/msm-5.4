			vout0_pins: vout0@0 {
				reg = <0>;
				amb,pinmux-ids = <0x1075 0x1076 0x1077 0x1078 0x1079
								0x107a 0x107b 0x107c 0x107d
								0x107e 0x107f 0x1080 0x1081
								0x1082 0x1083 0x1084 0x1085
								0x1086 0x1087 0x1088>;
				amb,pinconf-ids = <0xba0075 0xba0076 0xba0077 0xba0078 0xba0079
								0xba007a 0xba007b 0xba007c 0xba007d
								0xba007e 0xba007f 0xba0080 0xba0081
								0xba0082 0xba0083 0xba0084 0xba0085
								0xba0086 0xba0087 0xba0088>;
			};

		};
	};

	n_ahb@e0000000 { /* Non-Secure AHB, but configurable */
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0xe0000000 0x01000000>;
		ranges;

		spinor0: spinor@e0001000 {
			compatible = "ambarella,spinor";
			reg = <0xe0001000 0x1000>; /* spi nor controller */
			interrupts = <0 73 0x4>;
			pinctrl-names = "default";
			pinctrl-0 = <&spinor0_pins>;
			clocks = <&gclk_ssi3>;
