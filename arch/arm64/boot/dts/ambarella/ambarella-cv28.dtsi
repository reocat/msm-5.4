		};

		/* node for vin1 controller */
		vinc1 {
			compatible = "ambarella,vinc";
			interrupts = <0 135 0x1 0 136 0x1 0 137 0x1>;
			interrupt-names = "idsp", "idsp_sof", "idsp_last_pixel";
